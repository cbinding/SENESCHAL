	</skos:Concept>
	<skos:Concept rdf:about="http://purl.org/heritagedata/schemes/agl_et/concepts/145130">
		<skos:inScheme rdf:resource="http://purl.org/heritagedata/schemes/agl_et" />
		<dcterms:identifier>http://purl.org/heritagedata/schemes/agl_et/concepts/145130</dcterms:identifier>
		<skos:prefLabel xml:lang="en">FIELD OBSERVATION (MONITORING)</skos:prefLabel>
		<dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2013-07-17T08:43:37</dcterms:issued>
		<skos:broader rdf:resource="http://purl.org/heritagedata/schemes/agl_et/concepts/145109" />
		<skos:related rdf:resource="http://purl.org/heritagedata/schemes/agl_et/concepts/145177" />
		<skos:scopeNote xml:lang="en">Periodic visits being made to a site by an archaeologist to record archaeological deposits and recover finds that have come to light since the previous visit. An archaeological presence is not maintained during groundwork as it is with a watching brief.</skos:scopeNote>
		<cc:license rdf:resource="http://creativecommons.org/licenses/by/3.0" />
		<cc:attributionURL rdf:resource="http://www.english-heritage.org.uk" />
		<cc:attributionName xml:lang="en">English Heritage</cc:attributionName>
		<dcterms:publisher rdf:resource="http://www.english-heritage.org.uk" />
	</skos:Concept>
	<skos:Concept rdf:about="http://purl.org/heritagedata/schemes/agl_et/concepts/145131">
		<skos:inScheme rdf:resource="http://purl.org/heritagedata/schemes/agl_et" />
		<dcterms:identifier>http://purl.org/heritagedata/schemes/agl_et/concepts/145131</dcterms:identifier>
		<skos:prefLabel xml:lang="en">FIELD SURVEY</skos:prefLabel>
		<dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2013-07-17T08:43:37</dcterms:issued>
		<skos:narrower rdf:resource="http://purl.org/heritagedata/schemes/agl_et/concepts/147244" />
		<skos:narrower rdf:resource="http://purl.org/heritagedata/schemes/agl_et/concepts/145110" />
		<skos:narrower>
			<skos:Concept rdf:about="http://purl.org/heritagedata/schemes/agl_et/concepts/145113">
				<skos:inScheme rdf:resource="http://purl.org/heritagedata/schemes/agl_et" />
				<dcterms:identifier>http://purl.org/heritagedata/schemes/agl_et/concepts/145113</dcterms:identifier>
				<skos:prefLabel xml:lang="en">BOSING SURVEY</skos:prefLabel>
				<dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2013-07-17T08:43:37</dcterms:issued>
				<skos:broader rdf:resource="http://purl.org/heritagedata/schemes/agl_et/concepts/145131" />
				<skos:scopeNote xml:lang="en">A subsurface detection method performed by striking the ground with a heavy wooden mallet or a lead-filled container on a long handle.</skos:scopeNote>
				<cc:license rdf:resource="http://creativecommons.org/licenses/by/3.0" />
				<cc:attributionURL rdf:resource="http://www.english-heritage.org.uk" />
				<cc:attributionName xml:lang="en">English Heritage</cc:attributionName>
				<dcterms:publisher rdf:resource="http://www.english-heritage.org.uk" />
			</skos:Concept>
		</skos:narrower>
		<skos:narrower rdf:resource="http://purl.org/heritagedata/schemes/agl_et/concepts/145118" />
		<skos:narrower>
			<skos:Concept rdf:about="http://purl.org/heritagedata/schemes/agl_et/concepts/145124">
				<skos:inScheme rdf:resource="http://purl.org/heritagedata/schemes/agl_et" />
				<dcterms:identifier>http://purl.org/heritagedata/schemes/agl_et/concepts/145124</dcterms:identifier>
				<skos:prefLabel xml:lang="en">DOWSING SURVEY</skos:prefLabel>
				<dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2013-07-17T08:43:37</dcterms:issued>
				<skos:broader rdf:resource="http://purl.org/heritagedata/schemes/agl_et/concepts/145131" />
				<skos:scopeNote xml:lang="en">The supposed detection of subsurface features by employing a twig, copper rod, pendulum, or other instrument. Discontinuous movements in these instruments are believed by some to record the existence of buried features.</skos:scopeNote>
				<cc:license rdf:resource="http://creativecommons.org/licenses/by/3.0" />
				<cc:attributionURL rdf:resource="http://www.english-heritage.org.uk" />
				<cc:attributionName xml:lang="en">English Heritage</cc:attributionName>
				<dcterms:publisher rdf:resource="http://www.english-heritage.org.uk" />
			</skos:Concept>
		</skos:narrower>
		<skos:narrower rdf:resource="http://purl.org/heritagedata/schemes/agl_et/concepts/147317" />
		<skos:broader rdf:resource="http://purl.org/heritagedata/schemes/agl_et/concepts/147298" />
		<skos:narrower rdf:resource="http://purl.org/heritagedata/schemes/agl_et/concepts/146346" />
		<skos:narrower rdf:resource="http://purl.org/heritagedata/schemes/agl_et/concepts/152410" />
		<skos:narrower>
			<skos:Concept rdf:about="http://purl.org/heritagedata/schemes/agl_et/concepts/146341">
				<skos:inScheme rdf:resource="http://purl.org/heritagedata/schemes/agl_et" />
				<dcterms:identifier>http://purl.org/heritagedata/schemes/agl_et/concepts/146341</dcterms:identifier>
				<skos:prefLabel xml:lang="en">GEOMORPHOLOGICAL SURVEY</skos:prefLabel>
				<dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2013-07-17T08:43:37</dcterms:issued>
				<skos:broader rdf:resource="http://purl.org/heritagedata/schemes/agl_et/concepts/145131" />
				<skos:scopeNote xml:lang="en">Field survey undertaken to investigate land formation processes often carried out in association with, or in the vicinity of, an archaeological site, or as part of a wider landscape project.</skos:scopeNote>
				<cc:license rdf:resource="http://creativecommons.org/licenses/by/3.0" />
				<cc:attributionURL rdf:resource="http://www.english-heritage.org.uk" />
				<cc:attributionName xml:lang="en">English Heritage</cc:attributionName>
				<dcterms:publisher rdf:resource="http://www.english-heritage.org.uk" />
			</skos:Concept>
		</skos:narrower>
		<skos:narrower rdf:resource="http://purl.org/heritagedata/schemes/agl_et/concepts/145134" />
		<skos:narrower>
			<skos:Concept rdf:about="http://purl.org/heritagedata/schemes/agl_et/concepts/147306">
				<skos:inScheme rdf:resource="http://purl.org/heritagedata/schemes/agl_et" />
				<dcterms:identifier>http://purl.org/heritagedata/schemes/agl_et/concepts/147306</dcterms:identifier>
				<skos:prefLabel xml:lang="en">HEDGEROW SURVEY</skos:prefLabel>
				<dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2013-07-17T08:43:37</dcterms:issued>
				<skos:broader rdf:resource="http://purl.org/heritagedata/schemes/agl_et/concepts/145131" />
				<skos:scopeNote xml:lang="en">The assessment of the age of a hedge based on its nature, condition and the number and type of woody plant species present. Most surveys make use of Hooper's hypothesis which states that for every 27m a new woody species invades a hedge every 100 years.</skos:scopeNote>
				<cc:license rdf:resource="http://creativecommons.org/licenses/by/3.0" />
				<cc:attributionURL rdf:resource="http://www.english-heritage.org.uk" />
				<cc:attributionName xml:lang="en">English Heritage</cc:attributionName>
				<dcterms:publisher rdf:resource="http://www.english-heritage.org.uk" />
			</skos:Concept>
		</skos:narrower>
		<skos:narrower rdf:resource="http://purl.org/heritagedata/schemes/agl_et/concepts/147325" />
		<skos:narrower rdf:resource="http://purl.org/heritagedata/schemes/agl_et/concepts/146342" />
		<skos:narrower rdf:resource="http://purl.org/heritagedata/schemes/agl_et/concepts/145146" />
		<skos:narrower rdf:resource="http://purl.org/heritagedata/schemes/agl_et/concepts/145147" />
		<skos:narrower>
			<skos:Concept rdf:about="http://purl.org/heritagedata/schemes/agl_et/concepts/146345">
				<skos:inScheme rdf:resource="http://purl.org/heritagedata/schemes/agl_et" />
				<dcterms:identifier>http://purl.org/heritagedata/schemes/agl_et/concepts/146345</dcterms:identifier>
				<skos:prefLabel xml:lang="en">PHOTOGRAPHIC RECORDING</skos:prefLabel>
				<dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2013-07-17T08:43:37</dcterms:issued>
				<skos:broader rdf:resource="http://purl.org/heritagedata/schemes/agl_et/concepts/145131" />
				<skos:scopeNote xml:lang="en">The taking of photographs during informal site visits, which may be undertaken by amateur archaeologists or architectural students. More formal, detailed surveys should be termed Photographic Survey.</skos:scopeNote>
				<cc:license rdf:resource="http://creativecommons.org/licenses/by/3.0" />
				<cc:attributionURL rdf:resource="http://www.english-heritage.org.uk" />
				<cc:attributionName xml:lang="en">English Heritage</cc:attributionName>
				<dcterms:publisher rdf:resource="http://www.english-heritage.org.uk" />
			</skos:Concept>
		</skos:narrower>
		<skos:narrower rdf:resource="http://purl.org/heritagedata/schemes/agl_et/concepts/145154" />
		<skos:narrower>
			<skos:Concept rdf:about="http://purl.org/heritagedata/schemes/agl_et/concepts/147326">
				<skos:inScheme rdf:resource="http://purl.org/heritagedata/schemes/agl_et" />
				<dcterms:identifier>http://purl.org/heritagedata/schemes/agl_et/concepts/147326</dcterms:identifier>
				<skos:prefLabel xml:lang="en">THEMATIC SURVEY</skos:prefLabel>
				<dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2013-07-17T08:43:37</dcterms:issued>
				<skos:broader rdf:resource="http://purl.org/heritagedata/schemes/agl_et/concepts/145131" />
				<skos:scopeNote xml:lang="en">The investigation of heritage assets along thematic lines, usually to inform the production of a thematic study. Such studies bring specialist insights to bear on the particular area of study, but may not address other aspects of the site's significance.</skos:scopeNote>
				<cc:license rdf:resource="http://creativecommons.org/licenses/by/3.0" />
				<cc:attributionURL rdf:resource="http://www.english-heritage.org.uk" />
				<cc:attributionName xml:lang="en">English Heritage</cc:attributionName>
				<dcterms:publisher rdf:resource="http://www.english-heritage.org.uk" />
			</skos:Concept>
		</skos:narrower>
		<skos:narrower rdf:resource="http://purl.org/heritagedata/schemes/agl_et/concepts/145174" />
		<skos:narrower>
			<skos:Concept rdf:about="http://purl.org/heritagedata/schemes/agl_et/concepts/163245">
				<skos:inScheme rdf:resource="http://purl.org/heritagedata/schemes/agl_et" />
				<dcterms:identifier>http://purl.org/heritagedata/schemes/agl_et/concepts/163245</dcterms:identifier>
				<skos:prefLabel xml:lang="en">UNDERWATER SURVEY</skos:prefLabel>
				<dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2013-07-17T08:43:37</dcterms:issued>
				<skos:broader rdf:resource="http://purl.org/heritagedata/schemes/agl_et/concepts/145131" />
				<skos:scopeNote xml:lang="en">The non intrusive processes by which information is collected about the location, distribution and organisation of past human activities that are now submerged by water. The approaches can be either extensive or intensive depending on the objectives.</skos:scopeNote>
				<cc:license rdf:resource="http://creativecommons.org/licenses/by/3.0" />
				<cc:attributionURL rdf:resource="http://www.english-heritage.org.uk" />
				<cc:attributionName xml:lang="en">English Heritage</cc:attributionName>
				<dcterms:publisher rdf:resource="http://www.english-heritage.org.uk" />
			</skos:Concept>
		</skos:narrower>
		<skos:narrower rdf:resource="http://purl.org/heritagedata/schemes/agl_et/concepts/147322" />
		<skos:scopeNote xml:lang="en">The non intrusive processes by which information is collected about the location, distribution and organisation of past human activities. The approaches can be either extensive or intensive depending the objectivies that need to be addressed.</skos:scopeNote>
		<cc:license rdf:resource="http://creativecommons.org/licenses/by/3.0" />
		<cc:attributionURL rdf:resource="http://www.english-heritage.org.uk" />
		<cc:attributionName xml:lang="en">English Heritage</cc:attributionName>
		<dcterms:publisher rdf:resource="http://www.english-heritage.org.uk" />
	</skos:Concept>
