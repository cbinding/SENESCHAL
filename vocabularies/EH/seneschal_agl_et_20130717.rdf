				<dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2013-07-17T08:43:37</dcterms:issued>
				<skos:broader rdf:resource="http://purl.org/heritagedata/schemes/agl_et/concepts/147265" />
				<skos:scopeNote xml:lang="en">An underwater technique used to create 3D terrain models and to record objects on the seabed through the comparative analysis of reflections from high frequency sound waves emitted in multiple beams allowing large swathes of the seabed to be surveyed.</skos:scopeNote>
				<cc:license rdf:resource="http://creativecommons.org/licenses/by/3.0" />
				<cc:attributionURL rdf:resource="http://www.english-heritage.org.uk" />
				<cc:attributionName xml:lang="en">English Heritage</cc:attributionName>
				<dcterms:publisher rdf:resource="http://www.english-heritage.org.uk" />
