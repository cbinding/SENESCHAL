		<dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2013-07-17T10:08:39</dcterms:issued>
		<skos:narrower>
			<skos:Concept rdf:about="http://purl.org/heritagedata/schemes/3/concepts/501940">
				<skos:inScheme rdf:resource="http://purl.org/heritagedata/schemes/3" />
				<dcterms:identifier>http://purl.org/heritagedata/schemes/3/concepts/501940</dcterms:identifier>
