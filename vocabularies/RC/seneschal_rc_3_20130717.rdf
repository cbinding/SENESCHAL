		<dcterms:identifier>http://purl.org/heritagedata/schemes/3/concepts/501905</dcterms:identifier>
		<cc:license rdf:resource="http://reference.data.gov.uk/id/open-government-licence" />
		<cc:attributionURL rdf:resource="http://www.rcahms.gov.uk" />
		<cc:attributionName xml:lang="en">RCAHMS</cc:attributionName>
		<dcterms:publisher rdf:resource="http://www.rcahms.gov.uk" />
		<skos:prefLabel xml:lang="en">HOSPITAL SHIP</skos:prefLabel>
		<dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2013-07-17T10:08:39</dcterms:issued>
		<skos:broader rdf:resource="http://purl.org/heritagedata/schemes/3/concepts/501943" />
		<skos:broader rdf:resource="http://purl.org/heritagedata/schemes/3/concepts/501948" />
		<skos:scopeNote xml:lang="en">A ship constructed and/or used to accommodate and treat casualties of military operations, usually under specific conditions of the Geneva Convention. Such vessels are normally of considerable size and carry distinctive markings; requisitioned passenger ships are frequently used.</skos:scopeNote>
	</skos:Concept>
	<skos:Concept rdf:about="http://purl.org/heritagedata/schemes/3/concepts/501909">
