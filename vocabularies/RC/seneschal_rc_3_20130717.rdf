				<skos:broader rdf:resource="http://purl.org/heritagedata/schemes/3/concepts/501850" />
				<skos:scopeNote xml:lang="en">Variant of the Brig having auxiliary steam or diesel propulsion for use in light airs or contrary winds.</skos:scopeNote>
			</skos:Concept>
		</skos:narrower>
		<skos:broader rdf:resource="http://purl.org/heritagedata/schemes/3/concepts/501987" />
