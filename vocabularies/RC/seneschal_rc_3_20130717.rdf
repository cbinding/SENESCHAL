				<cc:attributionURL rdf:resource="http://www.rcahms.gov.uk" />
				<cc:attributionName xml:lang="en">RCAHMS</cc:attributionName>
				<dcterms:publisher rdf:resource="http://www.rcahms.gov.uk" />
				<skos:prefLabel xml:lang="en">GABBART</skos:prefLabel>
				<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2011-08-19T08:45:42</dcterms:modified>
				<dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2013-07-17T10:08:39</dcterms:issued>
				<skos:broader rdf:resource="http://purl.org/heritagedata/schemes/3/concepts/501925" />
				<skos:scopeNote xml:lang="en">Double-ended and full-bodied sailing vessel common around Scotland and sized to pass through the central Scottish canals; typically gaff-rigged.</skos:scopeNote>
			</skos:Concept>
		</skos:narrower>
		<skos:narrower rdf:resource="http://purl.org/heritagedata/schemes/3/concepts/501901" />
