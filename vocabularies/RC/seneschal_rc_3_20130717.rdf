				<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2007-04-24T12:12:05</dcterms:modified>
				<dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2013-07-17T10:08:39</dcterms:issued>
				<skos:broader rdf:resource="http://purl.org/heritagedata/schemes/3/concepts/501968" />
				<skos:scopeNote xml:lang="en">A large and robust trading vessel of North European origin, developed from the Mediterranean Caravel and characterised by having three masts, large castles fore and aft and a mixture of square and lateen sails.</skos:scopeNote>
			</skos:Concept>
		</skos:narrower>
