				<dcterms:identifier>http://purl.org/heritagedata/schemes/3/concepts/501937</dcterms:identifier>
				<cc:license rdf:resource="http://reference.data.gov.uk/id/open-government-licence" />
				<cc:attributionURL rdf:resource="http://www.rcahms.gov.uk" />
				<cc:attributionName xml:lang="en">RCAHMS</cc:attributionName>
				<dcterms:publisher rdf:resource="http://www.rcahms.gov.uk" />
				<skos:prefLabel xml:lang="en">MOTOR TORPEDO BOAT</skos:prefLabel>
				<dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2013-07-17T10:08:39</dcterms:issued>
				<skos:broader rdf:resource="http://purl.org/heritagedata/schemes/3/concepts/502015" />
				<skos:scopeNote xml:lang="en">A fast but lightweight coastal warship (typically of planing hull form and wooden construction) intended to pose a specialised threat to heavier warships within a restricted range of operation.</skos:scopeNote>
			</skos:Concept>
		</skos:narrower>
